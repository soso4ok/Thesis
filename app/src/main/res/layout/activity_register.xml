<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="187dp"
        android:layout_height="242dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/full_logo" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="270dp"
        android:layout_height="211dp"
        android:layout_marginTop="12dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView">

        <EditText
            android:id="@+id/phoneNumber"
            android:layout_width="271dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/transment"
            android:drawableLeft="@drawable/ic_mail"
            android:drawablePadding="40px"
            android:fontFamily="@font/inter_regular"
            android:hint="login"
            android:maxLength="40"
            android:minHeight="48dp"
            android:paddingBottom="10px"
            android:textColor="@color/purple_500"
            android:textColorHint="@color/logreg"
            android:textSize="15sp" />

        <View
            android:id="@+id/view34567"
            android:layout_width="270dp"
            android:layout_height="2dp"
            android:background="@android:color/black"
            android:backgroundTint="@color/background_tem"
            android:rotationY="181" />


        <EditText
            android:id="@+id/editTextTextEmailAddress"
            android:layout_width="271dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/transment"
            android:drawableLeft="@drawable/ic_mail"
            android:drawablePadding="40px"
            android:fontFamily="@font/inter_regular"
            android:hint="gmail"
            android:inputType="textEmailAddress"
            android:maxLength="40"
            android:minHeight="48dp"
            android:paddingBottom="10px"
            android:textColor="@color/purple_500"
            android:textColorHint="@color/logreg"
            android:textSize="15sp" />

        <View
            android:id="@+id/view"
            android:layout_width="270dp"
            android:layout_height="2dp"
            android:background="@android:color/black"
            android:backgroundTint="@color/background_tem"
            android:rotationY="181" />

        <EditText
            android:id="@+id/editTextTextPassword"
            android:layout_width="229dp"
            android:layout_height="48dp"
            android:backgroundTint="@color/transment"
            android:drawableLeft="@drawable/ic_password"
            android:drawablePadding="50px"
            android:ems="10"
            android:fontFamily="@font/inter_regular"
            android:hint="password"
            android:inputType="textPassword"
            android:maxLength="16"
            android:paddingLeft="18px"
            android:paddingBottom="0px"
            android:textColor="@color/purple_500"
            android:textColorHint="@color/logreg"
            android:textSize="15sp" />

        <View
            android:id="@+id/view2"
            android:layout_width="230dp"
            android:layout_height="2dp"
            android:background="@android:color/black"
            android:backgroundTint="@color/background_tem" />

        <EditText
            android:id="@+id/editTextTextConPassword"
            android:layout_width="229dp"
            android:layout_height="48dp"
            android:backgroundTint="@color/transment"
            android:drawableLeft="@drawable/ic_password"
            android:drawablePadding="50px"
            android:ems="10"
            android:fontFamily="@font/inter_regular"
            android:hint="repeat password"
            android:inputType="textPassword"
            android:maxLength="16"
            android:paddingLeft="18px"
            android:paddingBottom="0px"
            android:textColor="@color/purple_500"
            android:textColorHint="@color/logreg"
            android:textSize="15sp" />

        <View
            android:id="@+id/view3"
            android:layout_width="230dp"
            android:layout_height="2dp"
            android:background="@android:color/black"
            android:backgroundTint="@color/background_tem" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/bungee_inline"
        android:text="@string/register"
        android:textColor="@color/old_rose"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="355dp"
        android:layout_height="114dp"
        android:layout_marginTop="52dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/register_btn"
            android:layout_width="201dp"
            android:layout_height="50dp"
            android:background="@drawable/login_btn"
            android:fontFamily="@font/inter_bold"
            android:textAllCaps="false"
            android:paddingTop="-40px"
            android:text="@string/register"
            android:textColor="#FFF"
            android:textSize="20sp"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />

        <Button
            android:id="@+id/backToLogin"
            android:layout_width="155dp"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/singup_btn"
            android:fontFamily="@font/inter_medium"
            android:textAllCaps="false"
            android:text="@string/Sing_In"
            android:textColor="@color/purple_500" />


    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>